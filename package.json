{
  "name": "obsidian-theme-ebullientworks",
  "version": "0.3.1",
  "private": true,
  "description": "An Obsidian Theme",
  "author": "Erin Schnabel",
  "license": "MIT",
  "homepage": "https://github.com/ebullient",
  "scripts": {
    "build": "npx sass --no-source-map src/ebullientworks.scss obsidian.css; npx sass --no-source-map src/tasks-snippet.scss tasks-snippet.css",
    "dev": "npx sass src/ebullientworks.scss vault/.obsidian/themes/Ebullientworks.css; npx sass --no-source-map src/tasks-snippet.scss vault/.obsidian/snippets/tasks-snippet.css",
    "version": "auto-changelog -p && git add CHANGELOG.md",
    "test-version": "auto-changelog -p"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ebullient/obsidian-theme-ebullientworks.git"
  },
  "bugs": {
    "url": "https://github.com/ebullient/obsidian-theme-ebullientworks/issues"
  },
  "devDependencies": {
    "auto-changelog": "^2.3.0",
    "sass": "^1.43.2"
  },
  "auto-changelog": {
    "backfillLimit": false,
    "commitLimit": false,
    "ignoreCommitPattern": "(changelog|.* README\\.).*"
  }
}
